#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ethereum-input-data-decoder/bin/ethereum_input_data_decoder" "$@"
else 
  exec node  "$basedir/../ethereum-input-data-decoder/bin/ethereum_input_data_decoder" "$@"
fi
