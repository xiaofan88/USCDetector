{
  "name": "detect-evm-proxy",
  "version": "1.0.1",
  "description": "Detect proxy contract kinds and their target addresses using an EIP-1193 compatible JSON-RPC request function",
  "repository": "https://github.com/ahmedali8/detect-evm-proxy",
  "author": "ahmedali8 <github.com/ahmedali8>",
  "license": "MIT",
  "main": "build/cjs/index.js",
  "typings": "build/cjs/index.d.ts",
  "module": "build/esm/index.js",
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "yarn build",
    "prebuild": "rimraf build",
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "check": "yarn check:prettier && yarn check:lint && yarn check:spelling",
    "check:lint": "eslint src test --ext .ts",
    "check:prettier": "prettier \"src/**/*.ts\" --list-different",
    "check:spelling": "cspell \"**\"",
    "fix": "yarn fix:prettier && yarn fix:lint",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "yarn jest"
  },
  "devDependencies": {
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/providers": "^5.6.8",
    "@trivago/prettier-plugin-sort-imports": "^3.3.1",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "cspell": "^6.2.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
